[local]
localhost ansible_connection=local

[staging]
localhost:2222 ansible_port=2222 ansible_connection=ssh ansible_ssh_user=ansible #ansible_ssh_pass=ansible ansible_sudo_pass=ansible

[production]
localhost:22 ansible_port=22 ansible_connection=ssh ansible_ssh_user=ansible #ansible_ssh_pass=ansible ansible_sudo_pass=ansible

[all:vars]
ansible_connection=ssh
host_key_checking = False
ANSIBLE_HOST_KEY_CHECKING=False
